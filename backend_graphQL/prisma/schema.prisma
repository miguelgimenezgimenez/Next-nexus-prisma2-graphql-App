generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model device {
  id         Int     @id @default(autoincrement())
  brand      String
  link       String?
  image      String?
  name       String
  dimensions String?
  os         String?
  storage    String?
}
